<div class="row">
  <h1>Admin Namespace</h1>

  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Admin?</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td> <%= user.first_name %></td>
      <td> <%= user.last_name %></td>
      <td> <%= user.email %></td>
      <td> <%= user.admin %></td>
      <td><a href="#" class="btn warning">Edit</a></td>
      <td><a href="#" class="btn danger">Delete</a></td>
    <tr>
    <% end -%>
  </table>
</div>
